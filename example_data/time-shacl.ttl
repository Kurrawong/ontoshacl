PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:DateTimeDescriptionShape
    a sh:NodeShape ;
    rdfs:isDefinedBy : ;
    sh:property 
        [
            a sh:PropertyShape ;
            sh:class xsd:gDay ;
            sh:path time:day ;
            sh:severity sh:Violation ;
        ] ,
        [
            a sh:PropertyShape ;
            sh:class xsd:gYear ;
            sh:path time:year ;
            sh:severity sh:Violation ;
        ] ,
        [
            a sh:PropertyShape ;
            sh:hasValue <http://www.opengis.net/def/uom/ISO-8601/0/Gregorian> ;
            sh:path time:hasTRS ;
            sh:severity sh:Violation ;
        ] ,
        [
            a sh:PropertyShape ;
            sh:class xsd:gMonth ;
            sh:path time:month ;
            sh:severity sh:Violation ;
        ] ;
    sh:targetClass time:DateTimeDescription ;
.

:DateTimeIntervalShape
    a sh:NodeShape ;
    rdfs:isDefinedBy : ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:class time:GeneralDateTimeDescription ;
            sh:message """
- The object of the time:hasDateTimeDescription property on a time:DateTimeInterval must be a time:GeneralDateTimeDescription""" ;
            sh:path time:hasDateTimeDescription ;
            sh:severity sh:Warning ;
        ] ;
    sh:targetClass time:DateTimeInterval ;
.

