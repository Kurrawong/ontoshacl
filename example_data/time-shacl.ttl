        [
            a sh:PropertyShape ;
            sh:class time:DayOfWeek ;
            sh:maxCount "1"^^xsd:nonNegativeInteger ;
            sh:message """
- A time:GeneralDateTimeDescription must not have more than 0 time:dayOfWeek statements
- The object of the time:dayOfWeek property on a time:GeneralDateTimeDescription must be a time:DayOfWeek""" ;
            sh:path time:dayOfWeek ;
            sh:severity sh:Violation ;
        ] ,
        [
            a sh:PropertyShape ;
